<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.akexorcist.progressnotification">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.akexorcist.progressnotification.ui.ApplyAccountActivity" />

        <service
            android:name="com.akexorcist.progressnotification.service.ApplyPremiumAccountService"
            android:enabled="true"
            android:exported="false" />

        <activity android:name="com.akexorcist.progressnotification.ui.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.akexorcist.progressnotification.ui.ResultActivity"></activity>
    </application>

</manifest>